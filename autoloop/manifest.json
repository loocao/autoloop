{
	"name": "Auto Loop",
	"version": "0.2.12",
	"minimum_chrome_version" : "18.0.1025.162",
	"manifest_version": 2,
	"description": "虾米Loop自动点赞chrome插件",
	"options_page": "options.html",
	"icons" : {
		"48" : "icon_48.png",
		"128" : "icon_128.png"
	},
	"page_action" : {
		"default_title": "Auto Loop",
		"default_icon": "icon_48.png"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"http://loop.xiami.com/room/*"
	],
	"homepage_url": "http://www.oncereply.me/auto-loop-chrome-extensions"
}